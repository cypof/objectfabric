<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<servlet>
		<servlet-name>objectfabricServlet</servlet-name>
		<servlet-class>servlet.server.ServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>objectfabricServlet</servlet-name>
		<url-pattern>/servlet.example/objectfabric</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>servlet.html</welcome-file>
	</welcome-file-list>
</web-app>
